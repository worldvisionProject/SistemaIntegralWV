@model IEnumerable<WordVision.ec.Web.Areas.Indicadores.Models.FaseProgramaAreaViewModel>

<table id="faseProgramaAreaTable" class="table table-striped" width="100%" cellspacing="0">
    <thead>
        <tr>
            <th hidden>
                @Html.DisplayNameFor(model => model.Id)
            </th>
            <th>
                Programa Área
            </th>
            <th>
                Proyecto Tecnico / Especial
            </th>
            <th>
                Tipo Proyecto
            </th>
            <th>
                Financiamiento
            </th>
            <th>
                Fase Proyecto
            </th>            
            <th>
                Fecha Inicio
            </th>
            <th>
                Fecha Fin
            </th>
            <th>
                Observacion
            </th>
            <th>
                Fecha Diseño
            </th>
            <th>
                Fecha Rediseño
            </th>
            <th>
                Fecha Transición
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Dip1)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Dip2)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Dip3)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Dip4)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Dip5)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Dip6)
            </th>
            <th>
                Estado
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td hidden>
                @Html.DisplayFor(modelItem => item.Id)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ProgramaArea.Descripcion)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ProyectoTecnico.NombreProyecto)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ProyectoTecnico.TipoProyecto.Nombre)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ProyectoTecnico.Financiamiento.Nombre)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.FaseProyecto.Nombre)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.FechaInicio)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.FechaFin)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Observacion)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.FechaDisenio)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.FechaRedisenio)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.FechaTransicion)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Dip1)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Dip2)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Dip3)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Dip4)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Dip5)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Dip6)
            </td>
            
            <td>
                @Html.DisplayFor(modelItem => item.Estado.Nombre)
            </td>
            <td>
                <div class="btn-group">
                    <a onclick="jQueryModalGet('/Indicadores/FaseProgramaArea/OnGetCreateOrEdit?id=@item.Id','Editar Fase Programa Área')" class="dropdown-item"><i class="fas fa-pencil-alt"></i>  Edit</a> 
                </div>
            </td>
        </tr>
}
    </tbody>
</table>

<script>
    $(document).ready(function () {
        $("#faseProgramaAreaTable").DataTable();
    });
</script>
